  <groupId>de.dagere.kopeme</groupId>
  <artifactId>kopememojo</artifactId>
  <version>0.2-SNAPSHOT</version>
  <goalPrefix>kopememojo</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>performancetest</goal>
      <description>A Mojo for executing standalone-performance-tests with maven</description>
      <requiresDependencyResolution>compile</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>test</phase>
      <implementation>de.kopeme.mojo.KoPeMeMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>session</name>
          <type>org.apache.maven.execution.MavenSession</type>
          <required>true</required>
          <editable>false</editable>
          <description>The current Maven session.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>true</editable>
          <description>The maven project.</description>
        </parameter>
        <parameter>
          <name>standardoutput</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Where the standard-output of the runs should be saved</description>
        </parameter>
      </parameters>
      <requirements>
        <requirement>
          <role>org.apache.maven.plugin.BuildPluginManager</role>
          <field-name>pluginManager</field-name>
        </requirement>
      </requirements>
    </mojo>
  </mojos>
